{
    "name": "formapro/psb-enqueue-producer",
    "description": "Enqueue Message Producer for Prooph Service Bus",
    "homepage": "http://getprooph.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Maksym Kotliar",
            "email": "kotlyar.maksim@gmail.com"
        }
    ],
    "keywords": [
        "prooph",
        "CQRS",
        "messaging",
        "enqueue"
    ],
    "require": {
        "php": "~5.6|~7.0",
        "prooph/common": "^4",
        "enqueue/enqueue": "^0.5",
        "react/promise": "^2.5"
    },
    "require-dev": {
        "prooph/service-bus": "dev-master",
        "enqueue/simple-client": "^0.5",
        "symfony/filesystem": "^2.1|^3",
        "enqueue/fs": "^0.5",
        "phpunit/phpunit": "~4.7",
        "fabpot/php-cs-fixer": "1.7.*"
    },
    "autoload": {
        "psr-4": {
            "Formapro\\Prooph\\ServiceBus\\Message\\Enqueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProophTest\\ServiceBus\\": "vendor/prooph/service-bus/tests/"
        }
    }
}
